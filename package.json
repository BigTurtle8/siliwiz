    "solid-record": "^0.2.1",
    "three": "^0.149.0",
    "vinxi": "^0.3.6"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{js,css}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16"
